<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.xtkj.tlx.entity">
	<class name="OrderDetail" table="OrderDetail">
		<id name="odID">
		</id>
		<property name="customerID"/>
		<property name="lineName"/>
		<property name="price"/>
		<property name="orderDate"/>
		<property name="travelDate"/>
		<property name="total"/>
		<property name="lineID"/>
		<property name="state"/>
	</class>

</hibernate-mapping>

